<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/itembarr"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    >
    <TextView
        android:id="@+id/bar_kode_berang_out"
        android:layout_width="100dp"
        android:fontFamily="@font/poppins_semibold"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:textColor="@color/textColor"
        android:text="@string/kode_barang"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/bar_stok_out"
        android:layout_width="100dp"
        android:fontFamily="@font/poppins_semibold"
        android:layout_height="wrap_content"
        android:textColor="@color/textColor"
        android:gravity="center"
        android:text="Stok Keluar"
        android:textSize="14sp" />
    <TextView
        android:id="@+id/ukuran_keluar"
        android:layout_width="100dp"
        android:fontFamily="@font/poppins_semibold"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:textColor="@color/textColor"
        android:text="@string/ukuran"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/bar_harga_jual_out"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:textColor="@color/textColor"
        android:fontFamily="@font/poppins_semibold"
        android:text="harga jual"
        android:textSize="14sp" />
    <TextView
        android:id="@+id/bar_harga_total_out"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:textColor="@color/textColor"
        android:fontFamily="@font/poppins_semibold"
        android:text="total harga"
        android:textSize="14sp" />


</LinearLayout>